{"remainingRequest":"C:\\Users\\Zezoca\\Desktop\\Uni\\4ano\\2sem\\PRC\\PRC2021_TP\\app_server\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Zezoca\\Desktop\\Uni\\4ano\\2sem\\PRC\\PRC2021_TP\\app_server\\src\\components\\Receita.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\Users\\Zezoca\\Desktop\\Uni\\4ano\\2sem\\PRC\\PRC2021_TP\\app_server\\src\\components\\Receita.vue","mtime":1624906270860},{"path":"C:\\Users\\Zezoca\\Desktop\\Uni\\4ano\\2sem\\PRC\\PRC2021_TP\\app_server\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Zezoca\\Desktop\\Uni\\4ano\\2sem\\PRC\\PRC2021_TP\\app_server\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000},{"path":"C:\\Users\\Zezoca\\Desktop\\Uni\\4ano\\2sem\\PRC\\PRC2021_TP\\app_server\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Zezoca\\Desktop\\Uni\\4ano\\2sem\\PRC\\PRC2021_TP\\app_server\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Receita.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwJA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Receita.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n    <div id=\"receita\" class=\"receita\">\r\n        <v-container v-if=\"receita\"> \r\n            <v-row>\r\n                <v-col>\r\n                    <v-img max-height=\"600px\" v-if=\"receita.imgPath\" :src=\"'http://localhost:7700/uploads/'+receita.imgPath\"></v-img>\r\n                    <v-img max-height=\"600px\" v-else src=\"../../public/default.png\"></v-img>\r\n                </v-col>\r\n            </v-row>   \r\n            <v-row align=\"center\" >\r\n                <v-col>\r\n                    <h1 style=\"color:#57a2bf\"> {{ receita.titulo }} </h1> \r\n                    <span> <b>Autor:</b> {{receita.autor}} </span>\r\n                </v-col>\r\n\r\n                <v-col sm=\"1\" align=\"right\">\r\n                    <v-row align=\"center\">\r\n                        <div v-if=\"token\">\r\n                            <div v-if=\"receita.gostos.includes(idUser)\">\r\n                                <v-icon @click=\"dislike(idUser)\" x-large color=\"red\">mdi-cards-heart</v-icon>\r\n                                <span style=\"margin-left:5px; font-size:20px\"> {{ receita.gostos.length }} </span>\r\n                            </div>\r\n                            <div v-else>\r\n                                <v-icon @click=\"like(idUser)\" x-large color=\"grey\">mdi-cards-heart</v-icon>\r\n                                <span style=\"margin-left:5px; font-size:20px\"> {{ receita.gostos.length }} </span>\r\n                            </div>\r\n                        </div>\r\n                        <div v-else>\r\n                            <v-tooltip left max-width=\"160px\">\r\n                              <template v-slot:activator=\"{ on, attrs }\">\r\n                                <v-icon v-on=\"on\" v-bind=\"attrs\" x-large color=\"grey\">mdi-cards-heart</v-icon>\r\n                                <span style=\"margin-left:5px; font-size:20px\"> {{ receita.gostos.length }} </span>\r\n                              </template>\r\n                              <span>Efetue login para gostar das receitas</span>\r\n                            </v-tooltip>\r\n                            \r\n                        </div>\r\n                    </v-row>\r\n                </v-col>\r\n                \r\n            </v-row>\r\n\r\n            <v-card style=\"margin-top:10px;margin-bottom:10px\" color=\"#57a2bf\" height=\"2px\">\r\n                <v-card-title >\r\n                </v-card-title>\r\n            </v-card>\r\n\r\n            <v-row>\r\n               <v-col cols=\"7\">\r\n                   <span> <b> Dificuldade:</b>  \r\n                        <span v-if=\"receita.dificuldade=='Fácil'\" style=\"color:green\"> {{receita.dificuldade}} </span>\r\n                        <span  v-else-if=\"receita.dificuldade=='Média'\" style=\"color:orange\"> {{receita.dificuldade}} </span>\r\n                        <span v-else style=\"color:red\"> {{receita.dificuldade}} </span>\r\n                   </span>\r\n                   <span style=\"margin-right:10px;margin-left:10px\">|</span>\r\n                   <span> <b> Tipo de cozinha:</b> {{receita.tipoCozinha}} </span>\r\n                   <span style=\"margin-right:10px;margin-left:10px\">|</span>\r\n                   <span> <b> Tipo de prato:</b> {{receita.tipoPrato}} </span>\r\n                   \r\n               </v-col>\r\n               <v-col  align=\"right\">\r\n                   <v-btn style=\"margin-right:10px\" elevation=\"4\" @click=\"handleClick('/publicacoes?receita=' + receita.rec_id)\">\r\n                        Ver publicacoes\r\n                    </v-btn>\r\n                   <v-btn v-if=\"idUser==receita.autor_id\" elevation=\"4\" @click=\"removeReceita(receita.rec_id)\">\r\n                        Remover Receita\r\n                    </v-btn>\r\n               </v-col>\r\n            </v-row>\r\n\r\n            <v-row>\r\n                <v-col cols=\"8\">\r\n                    <v-card elevation=\"10\">\r\n                        <v-card-title>\r\n                            <h3> Descrição: </h3>\r\n                        </v-card-title>\r\n                        <v-card-text style=\"font-size:18px;max-width:650px\">\r\n                            {{receita.descricao}}\r\n                        </v-card-text>\r\n                    </v-card>\r\n                </v-col>\r\n                <v-col cols=\"4\">\r\n                    <v-card elevation=\"10\">\r\n                        <v-card-title>\r\n                            <h3> Ingredientes: </h3>\r\n                        </v-card-title>\r\n                        <v-card-text>\r\n                            <v-list v-for=\"i in receita.ingredientes\" :key=\"i\" style=\"padding:0px;margin:0px;\">\r\n                                <v-list-item dense style=\"padding:0px;margin:0px;font-size:18px\">\r\n                                  <v-icon color=\"#57a2bf\"> mdi-check </v-icon>\r\n                                  {{i}}\r\n                                </v-list-item>\r\n                            </v-list>\r\n                            \r\n                        </v-card-text>\r\n                    </v-card>\r\n                </v-col>\r\n            </v-row>\r\n            \r\n            <!--\r\n            <v-btn elevation=\"4\" @click=\"handleClick('/publicacoes?receita=' + receita.rec_id)\">\r\n                Ver publicacoes\r\n            </v-btn>-->\r\n\r\n            <v-card style=\"margin-top:20px;margin-bottom:20px\" color=\"#57a2bf\" height=\"2px\">\r\n                <v-card-title >\r\n                </v-card-title>\r\n            </v-card>\r\n\r\n            <v-row >\r\n              <v-col cols=\"12\" align=\"center\" justify=\"center\"> \r\n                <p v-if=\"token\" style=\"font-size:18px\"> Adiciona uma publicação sobre a receita! </p>\r\n                <p v-else style=\"font-size:18px\"> Efetua login para poderes adicionar publicações!</p>\r\n                <v-row>  \r\n                    <v-col align=\"center\" > \r\n                        <v-toolbar dense width=\"60.5rem\" elevation=\"6\">\r\n                            <v-text-field\r\n                             flat solo\r\n                             class=\"shrink\"\r\n                             autocomplete=\"off\"\r\n                             type=\"text\"\r\n                             :disabled=\"token?false:true\"\r\n                             hide-details\r\n                             v-model=\"titulo\"\r\n                             prepend-icon=\"mdi-pencil\"\r\n                             placeholder=\"Titulo\"    \r\n                           ></v-text-field>\r\n                           <v-text-field\r\n                             flat solo\r\n                             autocomplete=\"off\"\r\n                             type=\"text\"\r\n                             :disabled=\"token?false:true\"\r\n                             hide-details\r\n                             v-model=\"pub\"\r\n                             placeholder=\"Descrição\"\r\n                             prepend-icon=\"mdi-drag-vertical-variant\"        \r\n                           ></v-text-field>\r\n                            <v-btn :disabled=\"token?false:true\" @click=\"submitPub()\" style=\"margin-right:-10px\">\r\n                              Submeter\r\n                            </v-btn>\r\n                        </v-toolbar>\r\n                    </v-col>\r\n                </v-row>\r\n              </v-col>\r\n            </v-row>\r\n\r\n        </v-container>\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport jwt from 'jsonwebtoken'\r\n\r\nexport default {\r\n    name: 'receita',\r\n    data() {\r\n        return { \r\n            receita: null,\r\n            token: localStorage.getItem('jwt'),\r\n            pub:'',\r\n            titulo:'',\r\n            alerta: false,\r\n            idUser:''\r\n        }\r\n    },\r\n    created() {   \r\n        if (this.token) { \r\n            this.idUser = jwt.decode(this.token).email\r\n        }   \r\n       \r\n        axios.get(\"http://localhost:7700/receita/\"+this.$route.params.id)\r\n            .then(data => {\r\n                this.receita = data.data.receita\r\n            })\r\n            .catch(err => {\r\n                console.log(err)\r\n            })                 \r\n    },\r\n    methods: {\r\n        handleClick(value) {\r\n          this.$router.push(value)      \r\n        },\r\n        removeReceita(id) {\r\n          if (confirm(\"Deseja mesmo remover a receita?\")) {\r\n            var json={}\r\n            json['idReceita']= id\r\n            json['idUser']= this.idUser\r\n            console.log(id)\r\n            console.log(this.idUser)\r\n            axios({\r\n                method: \"post\",\r\n                url: \"http://localhost:7700/receita/remover\",\r\n                data: json,\r\n                headers: { \"Authorization\" : this.token},\r\n            })\r\n            .then(() => {\r\n                this.$router.push('/')\r\n            })\r\n            .catch(err => {\r\n                console.log(err)\r\n                alert('Não foi possível remover a receita')\r\n            })\r\n          }\r\n        },\r\n        sorted(lista) {\r\n            return lista.sort((a,b) => (a.data < b.data) ? 1 : ((b.data < a.data) ? -1 : 0))\r\n        },\r\n        like(id){\r\n            this.receita.gostos.push(id)\r\n            var json={}\r\n            json['idUser']= id\r\n            json['idReceita']= this.receita.rec_id\r\n            axios({\r\n                method: \"post\",\r\n                url: \"http://localhost:7700/receita/gostar\",\r\n                data: json,\r\n                headers: { \"Authorization\" : this.token},\r\n            })\r\n        },\r\n        dislike(id) {\r\n            var index = this.receita.gostos.map(function(item) { return item; }).indexOf(id);\r\n            this.receita.gostos.splice(index, 1);\r\n            var json={}\r\n            json['idUser']= id\r\n            json['idReceita']= this.receita.rec_id\r\n            axios({\r\n                method: \"post\",\r\n                url: \"http://localhost:7700/receita/desgostar\",\r\n                data: json,\r\n                headers: { \"Authorization\" : this.token},\r\n            })\r\n        },\r\n        limpar(){\r\n            this.titulo=''\r\n            this.descricao=''\r\n        },\r\n        submitPub(){ \r\n            var json={}\r\n            json['titulo']= this.titulo\r\n            json['descricao']= this.pub\r\n            json['idUser']= this.idUser\r\n            json['idReceita']= this.receita.rec_id\r\n            var x = (new Date()).getTimezoneOffset() * 60000; \r\n            var localISOTime = (new Date(Date.now() - x)).toISOString().slice(0,-1);  \r\n            json['data']= localISOTime.slice(0, 19).replace('T', ' ')\r\n            axios({\r\n                method: \"post\",\r\n                url: \"http://localhost:7700/publicacao/\",\r\n                data: json,\r\n                headers: { \"Authorization\" : this.token},\r\n            })\r\n            .then(() => {\r\n                this.limpar();\r\n                alert('Publicação foi adicionada com sucesso!')\r\n            })\r\n            .catch(err => {\r\n                console.log(err)\r\n                this.limpar();\r\n                alert('Não foi possível adicionar a nova publicação')\r\n            })\r\n        }\r\n    }\r\n}\r\n\r\n</script>\r\n\r\n\r\n\r\n\r\n<style>\r\n\r\n\r\n\r\n</style>"]}]}